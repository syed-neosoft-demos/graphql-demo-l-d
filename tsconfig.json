{
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",
    "outDir": "build",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*",
    "server.ts"
  ],
  "exclude": [
    "src/types/*",
  ]
}